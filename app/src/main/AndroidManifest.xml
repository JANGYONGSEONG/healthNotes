<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jang.yongs.healthnotes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jang.yongs.healthnotes.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jang.yongs.healthnotes.popup.MealPlanPopup"
            android:screenOrientation="portrait"
            android:theme="@style/MyPopupTheme" />
        <activity
            android:name="com.jang.yongs.healthnotes.popup.BodyCompositionPopup"
            android:screenOrientation="portrait"
            android:theme="@style/MyPopupTheme" />
        <activity android:name="com.jang.yongs.healthnotes.MealPlanActivity" />
        <activity android:name="com.jang.yongs.healthnotes.ExerciseSelectActivity" />
        <activity android:name="com.jang.yongs.healthnotes.ExerciseDetailActivity" />
        <activity android:name="com.jang.yongs.healthnotes.DoExerciseActivity" />
    </application>

</manifest>